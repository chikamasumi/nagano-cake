<div class="col-md-8 offset-md-1">
  <h4>注文履歴一覧</h4>
  <table>
    <thead>
      <th width="80" heigth="24">購入日時</th>
      <th width="300" heigth="24">購入者</th>
      <th width="200" heigth="24">注文個数</th>
    </thead>
    <tbody>
      <% @orders.each do |oder| %>
      <tr>
        <td>
          <%= link_to admin_order_path(order.id) do %>
            <%= order.created_at %>
          <% end %>
        </td>
        <td><%= oder.name %></td>
        <td><%= order_detail.amout %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>